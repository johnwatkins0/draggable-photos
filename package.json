{
  "name": "draggable-photos",
  "version": "1.0.5",
  "description": "A draggable slider for overlapping before/after images.",
  "main": "src/index.js",
  "scripts": {
    "start":
      "parcel example/src/index.html -d example -o --public-url /react-draggable-splitscreen/example/",
    "build":
      "parcel build example/src/index.html -d example --public-url /react-draggable-splitscreen/example/",
    "lint": "eslint src",
    "prepare": "npm run build",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwatkins0/draggable-photos.git"
  },
  "author": "John Watkins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johnwatkins0/draggable-photos/issues"
  },
  "homepage": "https://github.com/johnwatkins0/draggable-photos#readme",
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-draggable": "^3.0.4",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.14.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "parcel-bundler": "^1.3.1",
    "react-test-renderer": "^16.2.0"
  }
}
