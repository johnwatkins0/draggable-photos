{
  "name": "react-draggable-splitscreen",
  "version": "2.0.1",
  "description": "A React component for displaying overlapping elements with a draggable splitscreen-like effect.",
  "main": "src/index.js",
  "scripts": {
    "start": "parcel example/src/index.html -d example -o --public-url /react-draggable-splitscreen/example/",
    "build-example": "parcel build example/src/index.html -d example --public-url /react-draggable-splitscreen/example/",
    "build": "rm -rf dist && tsc",
    "lint": "tslint --project .",
    "prepare": "npm run build",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwatkins0/react-draggable-splitscreen.git"
  },
  "author": "John Watkins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johnwatkins0/react-draggable-splitscreen/issues"
  },
  "homepage": "https://github.com/johnwatkins0/react-draggable-splitscreen#readme",
  "dependencies": {
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-draggable": "^3.0.4",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "eslint-plugin-jest": "^22.1.2",
    "jest": "^22.0.4",
    "parcel-bundler": "^1.3.1",
    "prettier": "^1.15.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "files": [
    "dist/**/*"
  ]
}
